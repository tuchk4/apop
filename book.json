    "plugins": [ "theme-api", "timeline", "analytics", "runkit", "codetabs", "copy-code-button", "disqus"],
    "root": "./gitbook",
    "pluginsConfig": {
        "analytics": {
            "google": "UA-12666422-9"
        },
        "github": {
            "url": "https://github.com/marvelsrp/apop"
        },
