{
  "name": "rmk",
  "version": "2.1.0",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel -d ./ ./lib --comments=false --compact=true --minified",
    "dev": "babel -d ./dist ./lib --watch",
    "test": "jest",
    "test-dev": "jest --coverage --watch",
    "doc": "jsdoc --configure .jsdoc.json",
    "style": "jscs lib/*.js lib/**/*.js",
    "jsdoc2md:actions": "jsdoc2md lib/actions/*.js  lib/actions/**/*.js > docs/ACTIONS.md",
    "jsdoc2md:rmk": "jsdoc2md lib/index.js > docs/README.md",
    "jsdoc2md": "npm run jsdoc2md:actions",
    "github": "npm run jsdoc2md && git commit -a -m \"generate docs\" && git push",
    "format": "prettier --trailing-comma es5 --single-quote --write 'lib/**/*.js' '__tests__/**/*.js'",
    "pref": "babel-node tasks/bestResults.js"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "chalk": "^1.1.3",
    "cross-spawn": "^5.1.0",
    "jest": "^19.0.2",
    "jsdoc": "^3.4.3",
    "jsdoc-to-markdown": "^3.0.0",
    "prettier": "^0.22.0",
    "yargs": "^7.0.2"
  },
  "dependencies": {}
}
