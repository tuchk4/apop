{
  "name": "rmk",
  "version": "2.3.2",
  "main": "dist/rmk.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel-node rollup-build.js",
    "test": "jest",
    "test-dev": "jest --coverage --watch",
    "jsdoc2md": "npm run jsdoc2md:actions",
    "jsdoc2md:actions": "jsdoc2md lib/actions/*.js  lib/actions/**/*.js > docs/ACTIONS.md",
    "jsdoc2md:rmk": "jsdoc2md lib/index.js > docs/README.md",
    "github": "npm run jsdoc2md && git commit -a -m \"generate docs\" && git push",
    "style": "prettier --trailing-comma es5 --single-quote --write 'lib/**/**/*.js' '__tests__/**/*.js'",
    "pref": "npm run build && NODE_ENV=test babel-node tasks/bestResults.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babelrc-rollup": "^3.0.0",
    "chalk": "^2.0.1",
    "cross-spawn": "^5.1.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jsdoc": "^3.4.3",
    "jsdoc-to-markdown": "^3.0.0",
    "lint-staged": "^4.0.2",
    "prettier": "^1.5.3",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify": "^0.1.5",
    "uglify-es": "^3.0.18",
    "yargs": "^8.0.2"
  },
  "dependencies": {}
}
